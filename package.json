{
  "name": "remark-youtube",
  "version": "1.2.1",
  "description": "Remark plugin for youtube",
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "repository": "pkolt/remark-youtube",
  "scripts": {
    "test": "node --loader ts-node/esm --test --no-warnings ./src/*.test.ts",
    "clean": "rimraf ./dist",
    "build-esm": "tsc",
    "build-types": "tsc -p ./tsconfig.types.json",
    "build": "npm run clean && npm run build-esm && npm run build-types",
    "prepare": "npm run build",
    "format": "prettier --check src",
    "format-fix": "prettier --write src",
    "release": "release-it"
  },
  "keywords": [
    "remark",
    "rehype",
    "youtube",
    "iframe",
    "embedded"
  ],
  "bugs": {
    "url": "https://github.com/pkolt/remark-youtube/issues",
    "email": "pkoltyshev@gmail.com"
  },
  "author": "Pavel Koltyshev <pkoltyshev@gmail.com>",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 20.11.0"
  },
  "devDependencies": {
    "@types/mdast": "^3.0.11",
    "@types/node": "^20.11.0",
    "prettier": "^2.8.7",
    "rehype-stringify": "^9.0.3",
    "release-it": "^15.10.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unified": "^10.1.2"
  },
  "dependencies": {
    "unist-util-visit": "^4.1.2"
  },
  "peerDependencies": {
    "remark-rehype": "^10.1.0"
  },
  "packageManager": "yarn@4.1.0"
}
